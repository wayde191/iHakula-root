  <field access="public" class="jsx3.util.EventDispatcher" final="1" fullname="jsx3.util.EventDispatcher.SUBJECT_ALL" id="sfield:SUBJECT_ALL" name="SUBJECT_ALL" static="1" value="&quot;*&quot;"/>
  <method access="public" id="method:subscribe" name="subscribe">
    <param name="strEventId" text="the event type(s).">
      <type link="1" name="String"/>
      <type link="1" name="Array">
        <type link="1" name="String"/>
      </type>
    </param>
    <param name="objHandler" text="if an object, the instance to notify of events (objFunction is required); if a string, the JSX id of the instance to notify of events (objFunction is required), must exist in the same Server; if a function, the function to call to notify of events (objFunction ignored)">
      <type link="1" name="Object"/>
      <type link="1" name="String"/>
      <type name="function"/>
    </param>
    <param name="objFunction" text="if objHandler is a string or object then the function to call on that instance. either a function or a string that is the name of a method of the instance">
      <type name="function"/>
      <type link="1" name="String"/>
    </param>
    <throws text="if objHandler/objFunction are not a valid combination of types&#10;   ">
      <type link="1" name="jsx3.lang.IllegalArgumentException"/>
    </throws>
    <text>Subscribes an object or function to a type of event published by this object.
<p/>
As of version 3.4 a string value for <code>objHandler</code> is deprecated.</text>
  </method>
  <method access="public" id="method:unsubscribe" name="unsubscribe">
    <param name="strEventId" text="the event type(s).">
      <type link="1" name="String"/>
      <type link="1" name="Array">
        <type link="1" name="String"/>
      </type>
    </param>
    <param name="objHandler" text="the value of objHandler passed to subscribe&#10;   ">
      <type link="1" name="Object"/>
      <type link="1" name="String"/>
      <type name="function"/>
    </param>
    <text>Unsubscribe an object or function from an event published by this object.
<p/>
As of version 3.4 a string value for <code>objHandler</code> is deprecated.</text>
  </method>
  <method access="public" id="method:unsubscribeAll" name="unsubscribeAll">
    <param name="strEventId" text="the event type&#10;   ">
      <type link="1" name="String"/>
    </param>
    <text>Unsubscribes all subscribed objects to a type of event published by this object.</text>
  </method>
  <method access="public" id="method:publish" name="publish">
    <param name="objEvent" text="the event, should have at least a field 'subject' that is the event id, another common field is 'target' (target will default to this instance)">
      <type link="1" name="Object"/>
    </param>
    <return text="the number of listeners to which the event was broadcast">
      <type class="Number" link="1" name="int"/>
    </return>
    <throws text="objEvent is not an object with a &lt;code&gt;subject&lt;/code&gt; property&#10;   ">
      <type link="1" name="jsx3.lang.IllegalArgumentException"/>
    </throws>
    <text>Publishes an event to all subscribed objects.</text>
  </method>
